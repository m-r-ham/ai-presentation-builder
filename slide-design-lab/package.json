{
  "name": "slide-design-lab",
  "version": "1.0.0",
  "description": "Crowd-sourced slide design training lab for AI presentation builder",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "start": "cd backend && npm start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/m-r-ham/ai-presentation-builder.git",
    "directory": "slide-design-lab"
  },
  "keywords": [
    "slide-design",
    "ai-training",
    "presentation",
    "slidev",
    "crowd-sourcing"
  ],
  "author": "AI Presentation Builder Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}